{
    "name": "slim/slim-skeleton-js",
    "description": "A Slim Framework skeleton application for rapid development and rich feature",
    "keywords": ["microframework", "rest", "router", "psr7"],
    "homepage": "http://github.com/slimphp/Slim-Skeleton",
    "license": "MIT",
    "authors": [
        {
            "name": "Januar Sirait",
            "email": "januar.srt@gmail.com",
            "homepage": "https://github.com/januar/"
        }
    ],
    "require": {
        "slim/slim": "^3.0",
        "slim/php-view": "^2.0",
        "monolog/monolog": "^1.17",
        "symfony/console": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.8 < 6.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout" : 0
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "test": "phpunit"
    }
}
